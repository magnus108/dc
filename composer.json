        "drupal/metatag": "^1.0",
        "drupal/yoast_seo": "^1.2",
        "drupal/diff": "^1.0@RC"
    },
    "authors": [
        {
            "name": "Magnus MÃ¸ller",
            "email": "magnus108@me.com"
        }
    ],
